{
    "name": "rochdi/cmi-php",
    "description": "PHP package to communicate with the CMI payment plateform in Morocco",
    "keywords": [
        "cmi",
        "payment processing",
        "morocco payment",
        "cmi payment",
        "payment in morocco",
        "paiement en ligne cmi",
        "paiement au maroc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "mehdi rochdi",
            "email": "mehdi.rochdi@gmail.com",
            "homepage": "https://github.com/mehdirochdi/cmi-php/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "friendsofphp/php-cs-fixer": "2.17.1"
    },
    "autoload": {
        "psr-4": {
            "CMI\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CMI\\": [
                "tests/"
            ]
        }
    }
}
